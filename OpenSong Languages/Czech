  <welcome>Vítejte!</welcome>

  <songs_mode caption="Písně">Použijte tento mód, pokud chcete své písně zadat nebo upravit. Písně mohou být buď vytištěny nebo promítány jako snímky.
    <all_songs caption="Všechny písně">
      <print caption="Tisk">Vytiskne všechny dostupné písně. Tato vlastnost je výborná na tištění zpěvníku.</print>
      <export caption="Export">Vyexportuje všechny písně.</export>
      <find caption="Najít">Hledá zadané slovo či frázi ve všech písních.</find>
    </all_songs>
    <new_song caption="Nová píseň">
      <create caption="Vytvořit">Přidá do OpenSongu novou píseň.</create>
      <import caption="Import">Importuje do OpenSongu novou píseň z CCLI databáze.</import>
    </new_song>
    <selected_song caption="Vybraná píseň">
      <save caption="Uložit">Uloží vybranou píseň na disk.</save>
      <revert caption="Zpět">Obnoví aktuální píseň zpět na poslední uloženou verzi.</revert>
      <print caption="Tisk">Vytiskne aktuální píseň.</print>
      <present caption="Prezentace">Prezentuje aktuální píseň. Pokud jste provdeli změny v aktuální písni, budou uloženy.
        <single_screen caption="Jedna obrazovka"/>
        <dual_screen caption="Na projektor"/>
        <preview_dual_screen caption="Náhled"/>
      </present>
      <export caption="Export">Exportuje aktuální píseň.
        <to caption="Vyberte složku pro export."/>
      </export>
      <rename caption="Přejmenovat">Přejmenuje aktuální píseň.</rename>
      <move caption="Přesunout">Přesune aktuální píseň do  jiné složky.
        <where caption="Přesunout '%1' do složky:"/>
      </move>
      <delete caption="Smazat">Smaže aktuální píseň.</delete>
    </selected_song>

    <songs caption="Písně">Vyberte píseň z nabídky pro načtení do editoru.</songs>
    
    <curr_folder caption="Nachází se ve složce">Ukáže složku, která obsahuje tuto píseň (v závisloti ke složcevybrané v roletce Složka)</curr_folder>

    <song_folders caption="Složky">Vyberte složku z nabídky pro zúžení současné nabídky.
      <new>Vytvoří novou složku pro uložení písní.</new>
      <delete>Smaže právě vybranou složku písní. Složka musí být prázdná.</delete>
      <filter_all caption="Všechny"/>
      <filter_main caption="Hlavní"/>
    </song_folders>
  </songs_mode>

  <general_song_editor caption="Obecné">Zpřístupní u aktuální písně: název, podnadpis, autora, snímky a slova.
    <header caption="Editor písně"/>
    <show_general_editor caption="Obecné"/>
    <show_advanced_editor caption="Pokročilé"/>
    <title caption="Název">Nastaví název vybrané písně.</title>
    <author caption="Autor">Nastaví autora vybrané písně.</author>
    <copyright caption="Copyright">Nastaví copyright (autorská práva) vybrané písně.</copyright>
    <ccli caption="Verze/CCLI #">Nastaví verzi a zadavatele  písně (datum/verze/sbor/vkladatel) nebo číslo CCLI.</ccli>
    <hymn_number caption="Píseň číslo">Nastaví číslo vybrané písně.</hymn_number>
    <presentation caption="Prezentace">Nastaví pořadí prezentace vybrané písně. Oddíly by měly být odděleny mezerami. (např. "V1 C V2 C")</presentation>
    <lyrics caption="Slova/Akordy">Zde upravíte text pomocí nastavení Oddílů, Komentářů, Akordů a Slov.</lyrics>
    <insert caption="Vložit"/>
      <ins_section caption="Oddíl">Oddíly slouží k oddělení logických celků písně. (např. slok)
        <section_name caption="Název oddílu"/>
      </ins_section>
      <ins_lyrics caption="Slova/Akordy">Automaticky vloží úvodní znak pro akordy a slova.
        <number_of_lines caption="Počet řádků"/>
      </ins_lyrics>
      <ins_multi_verse caption="Více-slokový">Automaticky vloží úvodní znak pro akordy a slova pokud chcete použít více-slokový formát.
        <number_of_verses caption="Počet slok"/>
        <number_of_lines caption="Počet řádků"/>
      </ins_multi_verse>
      <ins_comment caption="Komentář">Komentáře jsou tištěny, ale nezobrazují se v prezentaci.</ins_comment>
      <ins_column caption="Nový sloupec">Vytištěné listy mohou obsahovat dva sloupce, pokud je píseň příliš dlouhá a nevejde se na stránku.</ins_column>
      <ins_page caption="Nová stránka">Označí místo, kde by mělo být vloženo zalomení stránky, pokud jsou tištěny delší písně.</ins_page>
    <transpose caption="Transpozice">Použijte tuto funkci pro automatickou transpozici písně do jakékoliv tóniny. Výška tranpozice je udávána v půltónech. 
      <using caption="Přednostně">Vyberte Křížky pro přednostní použití křížků při transpozici. Nebo vyber Béčka pro přednostní použití béček.
        <sharps caption="Křížky"/>
        <flats caption="Béčka"/>
      </using>
    </transpose>
  </general_song_editor>

  <advanced_song_editor caption="Pokročilé">Přístup k nastavení témat, kapu, taktu a stylu vybrané písně.
    <header caption="Editor písně"/>
    <show_general_editor caption="Obecné"/>
    <show_advanced_editor caption="Pokročilé"/>
    <themes caption="Témata">Nastaví hlavní témata vybrané písně.</themes>
    <capo caption="Kapo">Nastaví pozici kapa vybrané písně.</capo>
    <capo_print caption="Tisk">Pokud je tato možnost vybrána, OpenSong vytiskne kapo akordy spolu s ne-kapo akordy.</capo_print>
    <time_sig caption="Takt">Nastaví takt vybrané písně.</time_sig>
    <tempo caption="Tempo">Nastaví tempo vybrané písně.</tempo>
    <key caption="Klíč">Nastaví identifikační symbol písně.</key>
    <aka caption="Zkratkový název">Nastaví alternativní název písně.</aka>
    <key_line caption="Klíčová slova">Nastaví řádek, který vám pomůže píseň identifikovat.</key_line>
    <style caption="Přebít styl">Zatrhněte, pokud chcete použít vlastní styl. Vlastní styl přebije výchozí natavení stylu prezentace.</style>
    <user1 caption="Uživatelem definované 1">Zde nastavte, cokoli chcete. Toto pole může být použito programy třetích stran, atd.</user1>
    <user2 caption="Uživatelem definované 2">Zde nastavte, cokoli chcete. Toto pole může být použito programy třetích stran, atd.</user2>
    <user3 caption="Uživatelem definované 3">Zde nastavte, cokoli chcete. Toto pole může být použito programy třetích stran, atd.</user3>
  </advanced_song_editor>

  <sets_mode caption="Playlisty"> Tento mód použijte k přidání a úpravě svých playlistů. Playlisty se používají k promítnutí souboru písní, veršů a podobně.
    <sets caption="Playlisty">Vyberte playlist ze seznamu, abyste s ním mohli dále pracovat.
      <add>Přidá nový playlist do OpenSongu.</add>
    </sets>
    <current_set caption="Vybraný playlist">
      <save caption="Uložit">Uloží aktuální playlist (včetně všech položek) na disk.</save>
      <rename caption="Přejmenovat">Přejmenuje aktuální playlist.</rename>
      <delete caption="Smazat">Smaže aktuální playlist. (Písně v playlistu nebudou vymazány z disku ani seznamu písní.)</delete>
       <saveas caption="Uložit jako">Uloží aktuální playlist pod novým jménem.</saveas> 
      <present caption="Prezentace">Prezentuje aktuální píseň.
        <single_screen caption="Jedna obrazovka"/>
        <dual_screen caption="Na projektor"/>
        <preview_dual_screen caption="Náhled"/>
        <no_slides>Musíte mít alespoň jeden snímek.</no_slides>
      </present>
      <print_songs caption="Písně">Vytiskne jen písně v aktuálním playlistu.
        <no_songs>V playlistu není žádná píseň.</no_songs>
      </print_songs>
      <print_order caption="Položky">Vytiskne přehled položek v playlistu.</print_order>
      <export caption="Export">Exportuje tento playlist do jiné složky.</export>
    </current_set>
    <new_item caption="Nová položka">
      <add_song caption="Píseň">Přidá píseň do aktuálního playlistu. Nabídne seznam písní z Editoru písní.</add_song>
      <add_scripture caption="Bible">Přidá snímek s verši z Bible do aktuálního playlistu. Zobrazí se okno, které vám dovolí si vybrat verše, které si přejete zobrazit.</add_scripture>
      <add_slides caption="Snímky">Přidá uživatelský snímek do aktuálního playlistu. Uživatelské snímky vám dovolí vybrat si vlastní název, podnadpis, vlastní text a dokonce i styl. Uživatelský slide může ve skutečnosti obsahovat více snímků.</add_slides>
      <add_style caption="Styl">Mění aktuální základní styl kdekoli v playlistu. Změny stylů mohou definovat nový styl nebo navracet předchozí styl.</add_style>
      <add_video caption="Video">Přidá video do aktuálního playlistu. Použijte, pokud chcete prezentovat video uvnitř OpenSongu.</add_video>
      <add_powerpoint caption="PowerPoint">Přidá PowerPoint prezentaci do aktuálního playlistu. Použijte, pokud chcete ovládat PowerPoint prezentaci z OpenSongu.</add_powerpoint>
    </new_item>
    <current_item caption="Aktuální položka">
      <move_up caption="Nahoru">Přesune vybranou položku výše.</move_up>
      <move_down caption="Dolu">Přesune vybranou položku níže.</move_down>
      <remove caption="Odstranit">Odstraní vybranou položku z playlistu.</remove>
      <copy caption="Kopírovat">Vytvoří kopii vybrané položky.</copy>
      <store caption="Uložit">Uloží vybranou položku playlistu.</store>
    </current_item>
    <items caption="Položky playlistu">Klikněte na uživatelský snímek pro jeho otevření v editoru, dvojklik otevře píseň v editoru písní.
      <song caption="(píseň)"/>
      <custom caption="(uživatelský)"/>
      <scripture caption="(Bible)"/>
      <style caption="(styl)">-- Změnit styl --</style>
      <revert caption="(styl)">-- Navrátit styl --</revert>
      <defaultstyle caption="(styl)">-- Exportovaný styl --</defaultstyle>
    </items>
    <untitled caption="Nepojmenovaný"/>
    <errors>
      <revert caption="Chyba při znovunačtení playlistu &quot;%1&quot; ze souboru."/>
    </errors>
    <messages>
      <export_complete>&quot;%1&quot; exportováno do
%2
      </export_complete>
    </messages>
  </sets_mode>

  <slide_editor>
    <header caption="Editor snímku"/>
    <title caption="Název">Nastaví název snímku, který se zobrazí přímo ve snímku.</title>
    <subtitle caption="Podnadpis">Nastaví podnadpis snímku, který se zobrazí přímo ve snímku.</subtitle>
    <name caption="Název">Nastaví název snímku, který se zobrazí v okně Prezentační pomocník. Nezobrazí se přímo ve snímku.</name>
    <slides caption="Snímky">Obsahuje hlavní text aktuálního uživatelského snímku(ů).</slides>
    <presentation caption="Uživatelské pořadí prezentace">Přebije pořadí prezentace v písni.</presentation>
    <origorder caption="Původní pořadí prezentace">Pořadí prezentace v písni.</origorder>
    <folder caption="Složka s písní">Složka, kde se nachází vybraná položka.</folder>
    <notes caption="Poznámky">Tyto poznámky se zobrazí v Prezentačním pomocníku, pokud pouštíte prezentaci na více monitorů.</notes>
    <insert caption="Vložit"/>
      <ins_slide caption="Snímek">Určeno k vytvoření souboru snímků, které mají společný název, podnadpis, styl a/nebo automatické prohlížení. Snímky vytvořené touto cestou mohou mít také nastaveno promítání ve smyčce.</ins_slide>
    <loop caption="Smyčka">Zaškrtněte, pokud chcete, aby se OpenSong automaticky vrátil na první snímek této položky, když dojde k poslednímu.</loop>
    <print caption="Tisknout v seznamu">Odškrtněte, pokud nechcete, aby se tato skupina snímků tiskla v seznamu playlistu.</print>
    <auto_advance caption="Automatický přechod po:">Zadejte počet sekund, po které má být zobrazen každý snímek v souboru, než dojde k automatickému přechodu na další. (0=neaktivní)</auto_advance>
    <seconds caption="sekund"/>
    <style caption="Přebít styl">Zaškrtnutí umožní snímku, aby obsahoval uživatelský styl. Klikněte na náhled pro editaci stylu.</style>
  </slide_editor>

  <style_change>
    <header caption="Změna stylu"/>
