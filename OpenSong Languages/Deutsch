  <print_settings title="Druckeinstellungen">
    <tabs head3="Abstände" head1="Schriftarten" head2="Format"/>
    <margins>
      <left caption="Links"/>
      <top caption="Oben"/>
      <right caption="Rechts"/>
      <bottom caption="Unten"/>
