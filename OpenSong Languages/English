  <welcome>Welcome!</welcome>

  <songs_mode caption="Song Mode">Use this mode to enter and edit your songs. Songs can be printed as lead sheets, or presented as slides. When changing from songs mode to sets mode, you will be prompted to save any changes.
    <all_songs caption="All Songs">
      <print caption="Print">Prints all of the songs available. This feature is great for printing song books.</print>
      <export caption="Export">Exports all songs.</export>
      <find caption="Find">Searches the text of all the songs for a given word or phrase.</find>
    </all_songs>
    <new_song caption="New Song">
      <create caption="Create">Adds a new song to OpenSong.</create>
      <import caption="Import">Imports a new song into OpenSong from CCLI SongSelect.</import>
    </new_song>
    <selected_song caption="Selected Song">
      <save caption="Save">Writes the selected song to disk.</save>
      <revert caption="Revert">Restores the current song back to the version stored on disk.</revert>
      <print caption="Print">Prints the current song.</print>
      <present caption="Present">Presents the current song. If you have made changes to the current song it will be saved.
        <single_screen caption="Single Screen"/>
        <dual_screen caption="Dual Screen"/>
        <preview_dual_screen caption="Preview Dual Screen"/>
      </present>
      <export caption="Export">Exports the current song.</export>
      <rename caption="Rename">Renames the current song.</rename>
      <move caption="Move">Moves the current song into another folder.
        <where caption="Move '%1' to folder:"/>
      </move>
      <copy caption="Copy">Copies the current song to another folder.
        <where caption="Copy '%1' to folder:"/>
      </copy>
      <delete caption="Delete">Deletes the current song.</delete>
    </selected_song>

    <songs caption="Songs">Select a song from the list to load it into the song editor.</songs>

    <curr_folder caption="Containing Folder">Shows the folder containing this song (relative to the Active folder)</curr_folder>

    <song_folders caption="Folders">Select a folder from the list to filter the current list of songs.
      <new>Creates a new folder to store songs in.</new>
      <delete>Deletes the currently selected song folder. The folder must be empty.</delete>
      <filter_all caption="All"/>
      <filter_main caption="Main"/>
    </song_folders>
  </songs_mode>

  <general_song_editor caption="General">Accesses the current song's title, subtitle, author, slides, and lyrics.
    <header caption="Song Editor"/>
    <show_general_editor caption="General"/>
    <show_advanced_editor caption="Advanced"/>
    <title caption="Title">Sets the title of the selected song.</title>
    <author caption="Author">Sets the author of the selected song.</author>
    <copyright caption="Copyright">Sets the copyright of the selected song.</copyright>
    <ccli caption="CCLI Song #">Sets the CCLI number of the selected song.</ccli>
    <hymn_number caption="Hymn #">Sets the Hymn number of the selected song.</hymn_number>
    <presentation caption="Presentation">Sets the presentation order of the selected song. Sections should be separated with spaces. (i.e. "V1 C V2 C")</presentation>
    <lyrics caption="Chords/Lyrics">Edit songs here using Section lines, Comment lines, Chord lines, and Lyric lines.</lyrics>
    <insert caption="Insert"/>
      <ins_section caption="Section">Sections are used to divide parts of a song.
        <section_name caption="Section Name"/>
      </ins_section>
      <ins_lyrics caption="Chords/Lyrics">Automatically inserts the line prefixes for chords and lyrics.
        <number_of_lines caption="Number of Lines"/>
      </ins_lyrics>
      <ins_multi_verse caption="Multiple Verses">Automatically inserts the line prefixes for chords and lyrics if you want to use the multi-verse format.
        <number_of_verses caption="Number of Verses"/>
        <number_of_lines caption="Number of Lines"/>
      </ins_multi_verse>
      <ins_comment caption="Comment">Comments are printed on lead sheets but do not show up in presentations.</ins_comment>
      <ins_column caption="New Column">Lead sheets can contain two columns to aid in fitting longer songs onto a single page.</ins_column>
      <ins_page caption="New Page">Indicates the location a page break should be placed in the lead sheet when printing longer songs.</ins_page>
    <transpose caption="Transpose">Use this option to automatically transpose your song to any key. Amounts are measured in half-steps.
      <using caption="Using">Select Sharps from this list to cause OpenSong to transpose accidentals to sharps rather than flats. Select Flats from this list to cause OpenSong to transpose accidentals to flats rather than sharps.
        <sharps caption="Sharps"/>
        <flats caption="Flats"/>
      </using>
    </transpose>
  </general_song_editor>

  <advanced_song_editor caption="Advanced">Accesses the current song's theme, capo, time signature, style, etc.
    <header caption="Song Editor"/>
    <show_general_editor caption="General"/>
    <show_advanced_editor caption="Advanced"/>
    <themes caption="Themes">Sets the main theme of the selected song.</themes>
    <capo caption="Capo">Sets the capo position of the selected song.</capo>
    <capo_print caption="Print"> If this option is checked, OpenSong will print the capo chords in addition to the non-capo chords.</capo_print>
    <time_sig caption="Time Signature">Sets the time signature of the selected song.</time_sig>
    <tempo caption="Tempo">Sets the tempo of the selected song.</tempo>
    <key caption="Key">Sets the key signature of the selected song.</key>
    <aka caption="AKA">Sets the common alternate title of the selected song.</aka>
    <key_line caption="Key Line">Sets the line of the song that helps identify the selected song.</key_line>
    <style caption="Override Style">Click here to customize the style for this song. Custom styles override the default presentation style.</style>
    <user1 caption="User Defined 1">Set this to whatever you want it to be. This field can be used by 3rd party applications, etc.</user1>
    <user2 caption="User Defined 2">Set this to whatever you want it to be. This field can be used by 3rd party applications, etc.</user2>
    <user3 caption="User Defined 3">Set this to whatever you want it to be. This field can be used by 3rd party applications, etc.</user3>
  </advanced_song_editor>

  <sets_mode caption="Set Mode">Use this mode to enter and edit your sets. Sets are used to project a series of songs, slides, or whatever.  If currently in song mode and you have unsaved changes, you will be prompted to save.
    <sets caption="Sets">Select a set from the list to load it into the set editor.
      <add>Adds a new set to OpenSong.</add>
    </sets>
    <current_set caption="Current Set">
      <save caption="Save">Saves the current set (including all items) to disk.</save>
      <rename caption="Rename">Renames the current set.</rename>
      <delete caption="Delete">Deletes the current set. (Songs currently in this set will not be deleted from the song editor.)</delete>
       <saveas caption="Save As">Save the current set under a new name.</saveas>
      <present caption="Present">Presents the current set to the screen. You have the option of presenting to the current monitor, a secondary monitor, or using a single monitor to preview what it would be like with two monitors.
        <single_screen caption="Single Screen"/>
        <dual_screen caption="Dual Screen"/>
        <preview_dual_screen caption="Preview Dual Screen"/>
        <no_slides>You must have at least one slide in the set to present.</no_slides>
      </present>
      <print_songs caption="Songs">Prints only the songs inside of the current set.
        <no_songs>There are no songs in the set.</no_songs>
      </print_songs>
      <print_order caption="List">Prints an outline of this set.</print_order>
      <export caption="Export">Export this set to another folder.</export>
    </current_set>
    <new_item caption="New Item">
      <add_song caption="Song">Adds a song to the current set. This will present the list of songs available from the song editor for you to choose from.</add_song>
      <add_scripture caption="Scripture">Adds a scripture slide or slides to the current set. This will display a window that will let you choose the particular passage that you wish to display.</add_scripture>
      <add_slides caption="Slides">Adds a custom slide to the current set. Custom slides allow you to specify your own title, subtitle, body text, and even style. A custom slide item can actually contain a series of slides.</add_slides>
      <add_style caption="Style">Changes the current set's default style at any place in the set. Styles changes can define a new style or revert to the previous style.</add_style>
      <add_image caption="Image">Adds an image slide to the current set. Image slides will allow you to display a single image or a slideshow of multiple images instead of a body text.</add_image>
      <add_external caption="External">Adds a new slide to the current set that enables activation of external sources such as a presentation or DVD playback software.</add_external>
      <add_video caption="Video">Adds a video file to the current set. Use this to present a video from within OpenSong.</add_video>
      <add_powerpoint caption="PowerPoint">Adds a PowerPoint presentation to the current set. Use this to control a PowerPoint presentation from within OpenSong.</add_powerpoint>
    </new_item>
    <current_item caption="Current Item">
      <move_up caption="Move Up">Moves the currently selected item in the set closer to the beginning of the set.</move_up>
      <move_down caption="Move Dn">Moves the currently selected item in the set closer to the end of the set.</move_down>
      <remove caption="Remove">Removes the currently selected item from the set.</remove>
      <copy caption="Copy">Creates a copy of the currently selected set item.</copy>
      <paste caption="link(shared/insert/@caption)">Insert copied item after current position.</paste>      
      <store caption="Save">Stores the currently selected item in the set.</store>
      <presentitem caption="link(sets_mode/current_set/present/@caption)">Presents the current set to the screen, starting from the currently selected item. You have the option of presenting to the current monitor, a secondary monitor, or using a single monitor to preview what it would be like with two monitors.
        <single_screen caption="link(sets_mode/current_set/present/single_screen/@caption)"/>
        <dual_screen caption="link(sets_mode/current_set/present/dual_screen/@caption)"/>
        <preview_dual_screen caption="link(sets_mode/current_set/present/preview_dual_screen/@caption)"/>
        <no_slides>link(sets_mode/current_set/present/no_slides)</no_slides>
      </presentitem>
    </current_item>
    <items caption="Set Items">Lists the items in the current set. Click on a custom slide to load it into the custom slide editor or double-click a song to load it into the song editor.
      <song caption="(song)"/>
      <custom caption="(custom)"/>
      <scripture caption="(scripture)"/>
      <image caption="(image)"/>
      <external caption="(external)"/>
      <style caption="(style)">-- Change Style --</style>
      <revert caption="(style)">-- Revert Style --</revert>
      <defaultstyle caption="(style)">-- Exported Style --</defaultstyle>
    </items>
    <untitled caption="Untitled"/>
    <errors>
      <revert caption="Error reloading set &quot;%1&quot; from file."/>
    </errors>
    <messages>
      <export_complete>&quot;%1&quot; exported to
%2
      </export_complete>
    </messages>
  </sets_mode>

  <slide_editor>
    <header caption="Slide Editor"/>
    <title caption="Title">Sets the title of the slide displayed on the slide itself.</title>
    <subtitle caption="Subtitle">Sets the subtitle of the slide displayed on the slide itself.</subtitle>
    <name caption="Name">Sets the title of the slide(s) displayed in the Presentation Helper window. It does not display on the actual slide(s).</name>
    <slides caption="Slides">Contains the bodies of the actual slide(s) in this custom slide item. Hint: Drag one or more images to this list for easy adding.</slides>
    <presentation caption="Custom presentation order">Overrides the presentation order in the song file</presentation>
    <origorder caption="Original presentation order">Presentation order in the song file</origorder>       
    <folder caption="Song folder">Folder containing the selected item</folder>
    <notes caption="Notes">These notes show up in the Presentation Helper window while giving a presentation using multiple monitors.</notes>
    <insert caption="Insert"/>
      <ins_slide caption="Slide">Allows for the creation of multiple slides that all share a common title, subtitle, style, and/or auto-advance setting. Slides created this way can also be set to automatically loop.</ins_slide>
    <loop caption="Loop Slides">Check this box to have OpenSong automatically  loop back to the first slide in this item after it finishes displaying the last slide in this item.</loop>
    <print caption="Print in List">Uncheck this box to have this custom slide group not appear when printing set lists.</print>
    <transition caption="Use transition">Check this box to use a transition when this slide is shown.</transition>
    <auto_advance caption="Auto-advance slides every">Enter the number of seconds that OpenSong should display each slide in this item before automatically advancing to the next slide in this item.</auto_advance>
    <seconds caption="seconds"/>
    <style caption="Override Style">Checking this box enables this slide item to contain a custom style. When enabled, click the preview box above to edit this item's style.</style>
  </slide_editor>

  <image_slide>
    <header caption="Image Slide Editor"/>
    <title caption="link(slide_editor/title/@caption)">link(slide_editor/title)</title>
    <subtitle caption="link(slide_editor/subtitle/@caption)">link(slide_editor/subtitle)</subtitle>
    <descriptions caption="Image description in subtitle">Add the description of the showing image to the subtitle.</descriptions>
    <name caption="link(slide_editor/name/@caption)">link(slide_editor/name)</name>
    <images caption="Images" head1="Preview" head2="Description">Contains the images that are shown in this slide. Hint: Drag one or more images to this list for easy adding.</images>
    <fit_to_screen caption="Resize images to fit the screen">Images that are too large for the projector screen are resized to show the entire image. This also happens for images that are smaller than the screen size.</fit_to_screen>
    <fit_to_body caption="Resize images to fit the body area">Images are made to always fit in the area that is bounded by the body style settings. This goes for both images that are larger and smaller than the body area.</fit_to_body>
    <keepaspect caption="Keep aspect ratio when resizing images">In case of a resizing activity for either one of the above, the horizontal and vertical aspect will be maintained.</keepaspect>
    <store_as_link caption="Do not store the image in the set, use link to the image">Storing images in the set rapidly increases the size of it on disk and might degrade the quality of the image. Using a link avoids this. Images from the 'Backgrounds' folder are stored as relative link, which makes the switch to the presentation computer easier. Use with care if the presentation is created on a machine different from the presentation device!</store_as_link>
    <notes caption="link(slide_editor/notes/@caption)">link(slide_editor/notes)</notes>
    <action caption="Action" />
      <ins_image caption="Add">Adds an image to this slide to allow for a slideshow that shares a common title, subtitle, style, and/or auto-advance setting. Slides created this way can also be set to automatically loop.</ins_image>
      <imp_image caption="Import">Loads all available slides from a presentation and adds these as image.</imp_image>
      <del_image caption="Remove">Removes the currently selected image from the slide.</del_image>
      <move_up caption="Move up">Moves the currently selected image in the slide closer to the beginning of the slide.</move_up>
      <move_down caption="Move down">Moves the currently selected image in the slide closer to the end of the slide.</move_down>
    <loop caption="link(slide_editor/loop/@caption)">link(slide_editor/loop)</loop>
    <print caption="link(slide_editor/print/@caption)">link(slide_editor/print)</print>
    <transition caption="link(slide_editor/transition/@caption)">link(slide_editor/transition)</transition>
    <auto_advance caption="link(slide_editor/auto_advance/@caption)">link(slide_editor/auto_advance)</auto_advance>
    <seconds caption="link(slide_editor/seconds/@caption)">link(slide_editor/seconds)</seconds>
    <style caption="link(slide_editor/style/@caption)">link(slide_editor/style)</style>
  </image_slide>

