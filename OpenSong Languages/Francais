    </no_application>
    <presentation_settings caption="Paramètres de présentation">
      <presentation_host caption="Quelle application doit être utilisée pour lancer la présentation ?"/>
      <host_powerpoint caption="Microsoft PowerPoint"/>
      <host_powerpoint_info caption="Pour un contrôle maximal de votre présentation, une installation de Microsoft office sur votre ordinateur est recommandée."/>
      <host_pptview caption="Microsoft PowerPoint Viewer (visionneuse)"/>
      <host_pptview_info caption="Pour afficher votre présentation vous pouvez utiliser la visionneuse gratuite de PowerPoint. OpenSong essaiera de la détecter. Une visionneuse spécifique peut être paramétrée dans les paramètres généraux."/>
      <host_impress caption="OpenOffice.org Impress"/>
      <host_impress_info caption="Pour un contrôle maximal de votre présentation OpenOffice Impress, une installation de la suite OpenOffice est recommandée."/>
      <presentation_file caption="Fichier de présentation">Sélectionner le fichier de présentation en utilisant la fenêtre de sélection.</presentation_file>
      <loop_presentation caption="Présentation en boucle"/>
      <embed_presentation caption="Insérer la présentation dans le liste OpenSong"/>
    </presentation_settings>
    <videolan_settings caption="Paramètres Videolan">
