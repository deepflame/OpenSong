	  <videolan_mediafilename caption="Chemin vers le fichier multimédia">Choisir le fichier audio ou vidéo à lire.</videolan_mediafilename>
	  <videolan_preset caption="Utiliser le preset et les paramètres suivants"/>
	  <videolan_manual caption="Paramètres ajustés manuellement (utiliser %d pour le nombre d'affichage, %s pour le fichier multimedia)"/>
      <wait_for_videolan caption="Attendre que la lecture soit terminée avant de lancer la diapo suivante"/>
      <videolan_stop caption="Arrêter toutes les instances VideoLan lancées"/>
    </videolan_settings>
    <application_settings caption="Paramètres du lanceur d'application">
      <application_start caption="Lancer une application externe"/>
      <application_filename caption="Chemin vers l'exécutable de l'application">Choisir le fichier exécutable du programme qui doit être lancé.</application_filename>
      <application_parameters caption="Paramètres de l'application"/>
      <wait_for_application caption="Attendre que l'application soit terminée avant de continuer"/>
      <application_stop caption="Arrêter tout application lancée et toujours en cours."/>
    </application_settings>
    <notes caption="link(slide_editor/notes/@caption)">link(slide_editor/notes)</notes>
    <loop caption="link(slide_editor/loop/@caption)">link(slide_editor/loop)</loop>
  </external_slide>
  <style_change>
    <header caption="Changement de style"/>
    <revert caption="Revenir au style précédent">Cette option permet de revenir au style précédent à partir de ce point.</revert>
    <change caption="Changer le style en">Cette option permet de changer le style des listes à n'importe quel endroit de la liste. Un changement de style affecte toutes les diapositives qui suivent jusqu'au prochain changement de style.</change>
    <style>Cliquer ici pour modifier le nouveau style.</style>
