      <paragraph caption="Parágrafo"/>
      <verse caption="Verso"/>
    </format>
    <bad_reference>
      <empty>Referência ruim: a rferência estava vazia.</empty>
      <book>Referência ruim: livro não encontrado.</book>
      <version>Referência ruim: '%1' versão não encontrada.</version>
      <chapter>Referência ruim: não encontrou capítulo '%1'.</chapter>
      <verse>Referência ruim: não encontrou versículo '%1'.</verse>
      <verse_range>Referência ruim: o versículo 'to' deve ser igual ou maior o versículo 'from'.</verse_range>
    </bad_reference>
