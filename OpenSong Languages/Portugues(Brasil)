     <!--O termo "|NL|" é um código especial para informar ao OpenSong que cada legenda deve estar em linha separadas.-->
     <separator caption="|NL|"/>
     <!--A vírgula de entrelinhamento é necessária para dados internos por razões de estrutura.-->
     <sharpchords>,C,C#,D,D#,E,F,F#,G,G#,A,A#,B</sharpchords>
     <altsharpchords>,B#,C#,D,D#,E,E#,F#,G,G#,A,A#,B</altsharpchords>
     <flatchords>,C,Db,D,Eb,E,F,Gb,G,Ab,A,Bb,B</flatchords>
     <altflatchords>,C,Db,D,Eb,Fb,F,Gb,G,Ab,A,Bb,Cb</altflatchords>
  </songml>
  
  <smartml>
    <attrib_except>SmartML.GetAttribute: Exceção ao procurar por um atributo '%1' em '%2'</attrib_except>
    <no_path>Caminho não ancontrado.</no_path>
    <no_File>Arquivo não encontrado: %1</no_File>
