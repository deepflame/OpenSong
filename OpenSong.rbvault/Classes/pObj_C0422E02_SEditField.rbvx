  Enco/N: 1280
  name/S: mnu_edit_cut
  sorc/d: (Enco/N: 134217984
		Function mnu_edit_cut() As Boolean
		  Dim c As New Clipboard
		  If SelText.Len > 0 Then
		    c.SetText SelText
		    SelText = ""
		  End If
		  Return True
		End Function
  )
