  name/S: mnu_edit_copy
  sorc/d: (Enco/N: 134217984
		Function mnu_edit_copy() As Boolean
		  Dim c As New Clipboard
		  If SelText.Len > 0 Then
		    c.SetText SelText
		  End If
		  Return True
		End Function
  )
