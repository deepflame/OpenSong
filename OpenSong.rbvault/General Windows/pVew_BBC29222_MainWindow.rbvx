		        lst_songs_songs.ListIndex = listindex ' Open the next song in the list
		      ElseIf listindex = lst_songs_songs.ListCount Then
		        lst_songs_songs.ListIndex = listindex - 1 'Deleted the bottom of the list, stay there
		      End If
		      EnableMenuItems
		    End If
		  End If
