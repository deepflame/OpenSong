  Comp/S: 
  Vsbl/N: 1
  sorc/G: 
   Enco/N: 134217984
   srcl/S: Function mnu_settings_install_module() As Boolean
   srcl/S: 
   srcl/S: Dim file As FolderItem
   srcl/S: Dim newBible As Bible
   srcl/S: #if TargetMacOS
   srcl/S: file = GetOpenFolderItem("OpenSong Module (*.xmm)")
   srcl/S: #else
   srcl/S: file = GetOpenFolderItem(App.T.Translate("module/file_type"))
   srcl/S: #endif
   srcl/S: If file = Nil Then Return False
   srcl/S: 
   srcl/S: Dim toChild As String
   srcl/S: 
   srcl/S: Dim xfile As XmlDocument
   srcl/S: App.MouseCursor = WatchCursor
   srcl/S: xfile = SmartML.XDocFromFile(file)
   srcl/S: App.MouseCursor = Nil
   srcl/S: If xfile = Nil Then
   srcl/S: InputBox.Message App.T.Translate("module/read_error")
   srcl/S: Return False
   srcl/S: End If
   srcl/S: 
   srcl/S: If xfile.DocumentElement.Name = "bible" Then
   srcl/S: toChild = "OpenSong Scripture"
   srcl/S: ElseIf xfile.DocumentElement.Name = "language" Then
   srcl/S: toChild = "OpenSong Languages"
   srcl/S: Else
   srcl/S: InputBox.Message App.T.Translate("module/format_error")
   srcl/S: Return False
   srcl/S: End If
   srcl/S: 
   srcl/S: file.MoveFileTo App.AppFolder.Child(toChild)
   srcl/S: If file.LastErrorCode <> 0 Then
   srcl/S: InputBox.Message App.T.Translate("module/move_error", App.AppFolder.Child(toChild).AbsolutePath) + " (" + Str(file.LastErrorCode) + ")"
   srcl/S: Return False
   srcl/S: End If
   srcl/S: file = App.AppFolder.Child(toChild).Child(file.Name)
   srcl/S: If file = Nil Then
   srcl/S: InputBox.Message App.T.Translate("module/move_error", App.AppFolder.Child(toChild).AbsolutePath) + " (0)"
   srcl/S: Return False
   srcl/S: End If
   srcl/S: 
   srcl/S: If InStr(file.Name, ".") > 0 Then file.Name = Left(file.Name, StringUtils.InStrReverse(file.Name, ".") - 1)
   srcl/S: If file.LastErrorCode <> 0 Then
   srcl/S: InputBox.Message App.T.Translate("module/rename_error", App.AppFolder.Child(toChild).AbsolutePath)
   srcl/S: Return False
   srcl/S: End If
   srcl/S: 
   srcl/S: //++
   srcl/S: // If a Bible, offer to generate the index now.
   srcl/S: //--
   srcl/S: If xfile.DocumentElement.Name = "bible" Then
   srcl/S: If InputBox.AskYN(App.T.Translate("module/generate_index")) Then
   srcl/S: newBible = New Bible
   srcl/S: App.MouseCursor = WatchCursor
   srcl/S: If Not newBible.LoadBible(file) Then
   srcl/S: App.MouseCursor = Nil
   srcl/S: InputBox.Message(App.T.Translate("module/generate_error", file.AbsolutePath))
   srcl/S: Return True
   srcl/S: End If
   srcl/S: End If
   srcl/S: End If
   srcl/S: App.MouseCursor = Nil
   srcl/S: InputBox.Message App.T.Translate("module/success")
   srcl/S: Return True
   srcl/S: End Function
  sorc/g: 
