   srcl/S: ElseIf find.FindKey And InStr(Lowercase(SmartML.GetValue(s.DocumentElement, "key")), t) > 0 And (i <> lst_songs_songs.ListIndex Or FindFocus < 11 Or (FindFocus = 11 And (edt_song_key.SelStart+edt_song_key.SelLength = 0 Or InStr(edt_song_key.SelStart+edt_song_key.SelLength+1, Lowercase(edt_song_key.Text), t) > 0))) Then
   srcl/S: found = edt_song_key
   srcl/S: ElseIf find.FindAKA And InStr(Lowercase(SmartML.GetValue(s.DocumentElement, "aka")), t) > 0 And (i <> lst_songs_songs.ListIndex Or FindFocus < 12 Or (FindFocus = 12 And (edt_song_aka.SelStart+edt_song_aka.SelLength = 0 Or InStr(edt_song_aka.SelStart+edt_song_aka.SelLength+1, Lowercase(edt_song_aka.Text), t) > 0))) Then
   srcl/S: found = edt_song_aka
   srcl/S: ElseIf find.FindKeyLine And InStr(Lowercase(SmartML.GetValue(s.DocumentElement, "key_line")), t) > 0 And (i <> lst_songs_songs.ListIndex Or FindFocus < 13 Or (FindFocus = 13 And (edt_song_key_line.SelStart+edt_song_key_line.SelLength = 0 Or InStr(edt_song_key_line.SelStart+edt_song_key_line.SelLength+1, Lowercase(edt_song_key_line.Text), t) > 0))) Then
   srcl/S: found = edt_song_key_line
   srcl/S: ElseIf find.FindUser1 And InStr(Lowercase(SmartML.GetValue(s.DocumentElement, "user1")), t) > 0 And (i <> lst_songs_songs.ListIndex Or FindFocus < 14 Or (FindFocus = 14 And (edt_song_user1.SelStart+edt_song_user1.SelLength = 0 Or InStr(edt_song_user1.SelStart+edt_song_user1.SelLength+1, Lowercase(edt_song_user1.Text), t) > 0))) Then
   srcl/S: found = edt_song_user1
   srcl/S: ElseIf find.FindUser2 And InStr(Lowercase(SmartML.GetValue(s.DocumentElement, "user2")), t) > 0 And (i <> lst_songs_songs.ListIndex Or FindFocus < 15 Or (FindFocus = 15 And (edt_song_user2.SelStart+edt_song_user2.SelLength = 0 Or InStr(edt_song_user2.SelStart+edt_song_user2.SelLength+1, Lowercase(edt_song_user2.Text), t) > 0))) Then
   srcl/S: found = edt_song_user2
   srcl/S: ElseIf find.FindUser3 And InStr(Lowercase(SmartML.GetValue(s.DocumentElement, "user3")), t) > 0 And (i <> lst_songs_songs.ListIndex Or FindFocus < 16 Or (FindFocus = 16 And (edt_song_user3.SelStart+edt_song_user3.SelLength = 0 Or InStr(edt_song_user3.SelStart+edt_song_user3.SelLength+1, Lowercase(edt_song_user3.Text), t) > 0))) Then
   srcl/S: found = edt_song_user3
   srcl/S: '++JRC
   srcl/S: ElseIf find.FindHymnNumber And InStr(Lowercase(SmartML.GetValue(s.DocumentElement, "hymn_number")), t) > 0 And (i <> lst_songs_songs.ListIndex Or FindFocus < 17 Or (FindFocus = 17 And (edt_song_hymn_number.SelStart+edt_song_hymn_number.SelLength = 0 Or InStr(edt_song_hymn_number.SelStart+edt_song_hymn_number.SelLength+1, Lowercase(edt_song_hymn_number.Text), t) > 0))) Then
   srcl/S: found = edt_song_hymn_number
   srcl/S: '--
   srcl/S: End If
   srcl/S: 
   srcl/S: If found <> Nil Then
   srcl/S: If lst_songs_songs.ListIndex <> i Then lst_songs_songs.ListIndex = i
   srcl/S: found.SetFocus
   srcl/S: If found IsA EditField Then
   srcl/S: edtFound = EditField(found)
   srcl/S: If edtFound.SelStart+edtFound.SelLength = 0 Then
   srcl/S: edtFound.SelStart = InStr(Lowercase(edtFound.Text), t) - 1
   srcl/S: Else
   srcl/S: edtFound.SelStart = InStr(edtFound.SelStart+edtFound.SelLength+1, Lowercase(edtFound.Text), t) - 1
   srcl/S: End If
   srcl/S: edtFound.SelLength = find.FindString.Len
   srcl/S: End If
   srcl/S: 
   srcl/S: //++
   srcl/S: // 3 December 2006, EMP
   srcl/S: // Change logic that verifies the control is visible to a more generic form 
   srcl/S: // that doesn't rely on knowledge of the panel layout
   srcl/S: // (Note: the change from TabPanelIndex to PanelIndex may actually be with RB2005,
   srcl/S: // but I had no way to verify this.  Hopefully no one will be using the RB2005 compiler.)
   srcl/S: //
   srcl/S: #if RBVersion < 2006
   srcl/S: If pge_contents.Value <> Control(found).TabPanelIndex - 1 And Control(found).TabPanelIndex > 0 Then
   srcl/S: pge_contents.Value = Control(found).TabPanelIndex - 1
   srcl/S: End If
   srcl/S: #else
   srcl/S: If pge_contents.Value <> Control(found).PanelIndex And Control(found).PanelIndex > -1 Then
   srcl/S: pge_contents.Value = Control(found).PanelIndex
   srcl/S: End If
   srcl/S: #endif
   srcl/S: //--
   srcl/S: Else
   srcl/S: Do 
   srcl/S: i = i + 1
   srcl/S: If i >= lst_songs_songs.ListCount Then 
   srcl/S: i = 0 // Circle around to the beginning
   srcl/S: End If
   srcl/S: //
   srcl/S: //++ Check for finished
   srcl/S: //
   srcl/S: If i = FindStartSong Then Exit
   srcl/S: //--
   srcl/S: If i < lst_songs_songs.ListCount Then
   srcl/S: f = Songs.GetFile(lst_songs_songs.CellTag(i, 0).StringValue + lst_songs_songs.List(i))
   srcl/S: If f = Nil Or Not f.Exists Then
   srcl/S: App.MouseCursor = Nil
   srcl/S: InputBox.Message App.T.Translate("folderdb_errors/error[@code='"+Str(Songs.ErrorCode)+"']", lst_songs_songs.List(i))
   srcl/S: Return
   srcl/S: End If
   srcl/S: s = SmartML.XDocFromFile(f)
   srcl/S: If s = Nil Then
   srcl/S: App.DebugWriter.Write("MainWindow.FindNext: Error converting file to XML String: " + lst_songs_songs.CellTag(i, 0).StringValue + lst_songs_songs.list(i), 1)
   srcl/S: App.MouseCursor = Nil
   srcl/S: InputBox.Message App.T.Translate("errors/bad_format", lst_songs_songs.List(i))
   srcl/S: App.MouseCursor = WatchCursor
   srcl/S: End If
   srcl/S: End If
   srcl/S: Loop Until s <> Nil
   srcl/S: End If
   srcl/S: //++ 
   srcl/S: // See if we left the loop because we are at the end.  If so, leave the While loop
   srcl/S: //--
   srcl/S: If i = FindStartSong Then 
   srcl/S: Exit
   srcl/S: End If
   srcl/S: Wend
   srcl/S: 
   srcl/S: If found = Nil Then
   srcl/S: App.MouseCursor = Nil
   srcl/S: InputBox.Message App.T.Translate("errors/end_of_songs")
   srcl/S: End If
   srcl/S: 
   srcl/S: App.MouseCursor = Nil
   srcl/S: App.DebugWriter.Write("MainWindow.FindNext: Exit")
   srcl/S: Return
   srcl/S: End Sub
