   name/S: Action
   sorc/G: 
    Enco/N: 134217984
    srcl/S: Sub Action()
    srcl/S: Dim f As FolderItem
    srcl/S: Dim SongPath As String
    srcl/S: Dim FolderPath As String
    srcl/S: Dim Where As Integer
    srcl/S: 
    srcl/S: Dim xgroups, newGroup As XmlNode
    srcl/S: xgroups = SmartML.GetNode(CurrentSet.DocumentElement, "slide_groups", True)
    srcl/S: 
    srcl/S: Where = lst_set_items.ListIndex
    srcl/S: 
    srcl/S: f = SongPickerWindow.Popup
    srcl/S: If f <> Nil Then
    srcl/S: Status_SetChanged = True
    srcl/S: Status_InSetChanged = False
    srcl/S: ' No need to EnableMenuItems, since the following selection change will call it
    srcl/S: End If
    srcl/S: 
    srcl/S: While f <> Nil
    srcl/S: If lst_set_items.ListIndex >= 0 Then
    srcl/S: Where = lst_set_items.ListIndex + 1
    srcl/S: Else
    srcl/S: Where = lst_set_items.ListCount
    srcl/S: End If
    srcl/S: newGroup = SmartML.InsertChild(xgroups, "slide_group", Where)
    srcl/S: SmartML.SetValue newGroup, "@name", f.Name
    srcl/S: SmartML.SetValue newGroup, "@type", "song"
    srcl/S: //++
    srcl/S: // EMP, May 2006
    srcl/S: //
    srcl/S: // Add a path attribute relative to the base songs directory.  Otherwise, two instances
    srcl/S: // of the same song name in a parent/child folder relationship will never find the song
    srcl/S: // in the child folder.
    srcl/S: //
    srcl/S: SongPath = f.Parent.AbsolutePath
    srcl/S: FolderPath = MainWindow.Songs.GetRootFolder.AbsolutePath
    srcl/S: SongPath = Mid(SongPath, Len(FolderPath) + 1) //Take off the leading path separator
    srcl/S: // Change path separator to forward slant "/" to match separator in FolderDB
    srcl/S: // This is platform specific
    srcl/S: #If TargetWin32
    srcl/S: SongPath = ReplaceAll(SongPath, "\", "/")
    srcl/S: #ElseIf TargetMacOS
    srcl/S: SongPath = ReplaceAll(SongPath, ":", "/")
    srcl/S: #EndIf
    srcl/S: SmartML.SetValue newGroup, "@path", SongPath
    srcl/S: //--
    srcl/S: If lst_set_items.ListIndex >= 0 Then
    srcl/S: lst_set_items.InsertRow Where, f.Name + " "  + App.T.Translate("sets_mode/items/song/@caption")
    srcl/S: Else
    srcl/S: lst_set_items.AddRow f.Name + " " + App.T.Translate("sets_mode/items/song/@caption")
    srcl/S: End If
    srcl/S: 
    srcl/S: lst_set_items.ListIndex = Where
    srcl/S: f = SongPickerWindow.Popup
    srcl/S: Wend
    srcl/S: lst_set_items.ListIndex = Where
    srcl/S: End Sub
