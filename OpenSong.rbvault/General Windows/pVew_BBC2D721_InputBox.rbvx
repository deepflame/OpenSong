		Sub Message(message As String)
		  lbl_the_question.Text = message
		  edt_the_input.Visible = False
		  '++JRC: Increased height
		  lbl_the_question.Height = 150
		  '--
		  pop_the_input.Visible = False
		  
		  btn_ok.Left = 230
		  btn_cancel.Visible = False
		  
		  ShowModal
		  
		End Sub
