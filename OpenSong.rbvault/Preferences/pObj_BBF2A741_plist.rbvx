  sorc/d: (Enco/N: 134217984
		Sub ClearSearch(dict as plistDict)
		  Dim key,type,value as string
		  dim ok As Boolean
		  
		  dict.MoveFirst
		  While not dict.eof
		    key=dict.CurrentKey
		    type=dict.GetType(key)
		    If type="dict" or type="array" then
		      ClearSearch(dict.child(key))
		    else
		      dict.searched.Value(key)=false
		    end
		    dict.MoveNext
		  Wend
		  
		  
		End Sub
  )
