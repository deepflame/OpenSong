		Sub Dump()
		  dim c As clipboard
		  dim count As integer
		  dim s As string
		  
		  c=new clipboard
		  c.Text=""
		  
		  for count=1 to ubound(headers)
		    s=s+headers(count)+chr(13)
		  next
		  c.text=s
		  c.close
		  root.Dump(1)
		  c=new clipboard
		  s=s+"</dict>"+chr(13)
		  s=s+"</plist>"+chr(13)
		  c.text=c.text+s
		  c.close
		End Sub
  )
