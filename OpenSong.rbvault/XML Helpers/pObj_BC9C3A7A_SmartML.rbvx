		Sub CloneChildren(fromNode As XmlNode, toNode As XmlNode)
		  Dim xchild As XmlNode
		  xchild = fromNode.FirstChild
		  While xchild <> Nil
		    toNode.AppendChild toNode.OwnerDocument.ImportNode(xchild, True)
		    xchild = xchild.NextSibling
		  Wend
		End Sub
  )
