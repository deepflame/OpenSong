		Function GetValueC(xnode As XmlNode, childPath As String, ByRef c As Color, create As Boolean = True) As Boolean
		  Dim s As String
		  s = GetValue(xnode, childPath, create)
		  If Left(s, 1) <> "#" Or Len(s) <> 7 Then Return False
		  
		  c = RGB( _
		      Val("&H" + (Mid(s, 2, 2))), _
		      Val("&H" + (Mid(s, 4, 2))), _
		          Val("&H" + (Mid(s, 6, 2))))
		      
		      Return True
		    End Function
  )
