		Sub SetValueC(xnode As XmlNode, childPath As String, c As Color)
		  Dim r, g, b As String
		  
		  r = Hex(c.red)
		  If c.red < 16 Then r = "0" + r
		  g = Hex(c.green)
		  If c.green < 16 Then g = "0" + g
		  b = Hex(c.blue)
		  If c.blue < 16 Then b = "0" + b
		  
		  SetValue xnode, childPath, "#" + r + g + b
		End Sub
  )
