		Function KeyDown(Key As String) As Boolean
		  dim c As New Clipboard
		  
		  if strComp(Key, Chr(CTRL_V), 0) = 0 then 'ctrl-v
		    
		    if c.TextAvailable then
		      edt_background_trim_bottom.Text  = Left(c.Text, 4)
		      return true
		    end if
		    
		  end if
		End Function
   )
