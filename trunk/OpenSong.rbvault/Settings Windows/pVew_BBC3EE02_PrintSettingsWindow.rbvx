   name/S: Open
   sorc/G: 
    Enco/N: 134217984
    srcl/S: Sub Open()
    srcl/S: 
    srcl/S: Dim PS As XmlElement
    srcl/S: Dim Setup As PrinterSetup
    srcl/S: 
    srcl/S: PS = App.MyPrintSettings.DocumentElement
    srcl/S: Setup = App.GetPrinterSetup(False, Nil)
    srcl/S: 
    srcl/S: ValuesInPoints = SmartML.GetValueB(PS, "page/@points", False)
    srcl/S: 
    srcl/S: rad_page_inches.Value = SmartML.GetValueB(PS, "page/@inches", True, True)
    srcl/S: rad_page_cm.Value = Not rad_page_inches.Value
    srcl/S: SetupMarginMinimums(Setup, rad_page_inches.Value)
    srcl/S: 
    srcl/S: If ValuesInPoints Then
    srcl/S: If rad_page_inches.Value Then
    srcl/S: edt_page_top.SetValue  Max(PointsToInches(SmartML.GetValueN(PS, "page/@top")), edt_page_top.Minimum)
    srcl/S: edt_page_bottom.SetValue  Max(PointsToInches(SmartML.GetValueN(PS, "page/@bottom")), edt_page_bottom.Minimum)
    srcl/S: edt_page_left.SetValue  Max(PointsToInches(SmartML.GetValueN(PS, "page/@left")), edt_page_left.Minimum)
    srcl/S: edt_page_right.SetValue  Max(PointsToInches(SmartML.GetValueN(PS, "page/@right")), edt_page_right.Minimum)
    srcl/S: 
    srcl/S: If Setup <> Nil Then
    srcl/S: edt_page_width.SetValue Setup.PageWidth / Setup.HorizontalResolution
    srcl/S: edt_page_height.SetValue Setup.PageHeight / Setup.VerticalResolution
    srcl/S: Else
    srcl/S: edt_page_width.SetValue PointsToInches(SmartML.GetValueN(PS, "page/@width"))
    srcl/S: edt_page_height.SetValue PointsToInches(SmartML.GetValueN(PS, "page/@height"))
    srcl/S: End If
    srcl/S: 
    srcl/S: Else
    srcl/S: edt_page_top.SetValue  Max(PointsToCM(SmartML.GetValueN(PS, "page/@top")), edt_page_top.Minimum)
    srcl/S: edt_page_bottom.SetValue  Max(PointsToCM(SmartML.GetValueN(PS, "page/@bottom")), edt_page_bottom.Minimum)
    srcl/S: edt_page_left.SetValue  Max(PointsToCM(SmartML.GetValueN(PS, "page/@left")), edt_page_left.Minimum)
    srcl/S: edt_page_right.SetValue  Max(PointsToCM(SmartML.GetValueN(PS, "page/@right")), edt_page_right.Minimum)
    srcl/S: 
    srcl/S: If Setup <> Nil Then
    srcl/S: edt_page_width.SetValue PointsToCM(InchesToPoints(Setup.PageWidth / Setup.HorizontalResolution))
    srcl/S: edt_page_height.SetValue PointsToCM(InchesToPoints(Setup.PageHeight / Setup.VerticalResolution))
    srcl/S: Else
    srcl/S: edt_page_width.SetValue PointsToCM(SmartML.GetValueN(PS, "page/@width"))
    srcl/S: edt_page_height.SetValue PointsToCM(SmartML.GetValueN(PS, "page/@height"))
    srcl/S: End If
    srcl/S: End If
    srcl/S: Else
    srcl/S: edt_page_top.SetValue  SmartML.GetValueN(PS, "page/@top")
    srcl/S: edt_page_bottom.SetValue  SmartML.GetValueN(PS, "page/@bottom")
    srcl/S: edt_page_left.SetValue  SmartML.GetValueN(PS, "page/@left")
    srcl/S: edt_page_right.SetValue  SmartML.GetValueN(PS, "page/@right")
    srcl/S: If Setup <> Nil Then
    srcl/S: edt_page_width.SetValue Setup.PageWidth / Setup.HorizontalResolution
    srcl/S: edt_page_height.SetValue Setup.PageHeight / Setup.VerticalResolution
    srcl/S: Else
    srcl/S: edt_page_width.SetValue SmartML.GetValueN(PS, "page/@width")
    srcl/S: edt_page_height.SetValue SmartML.GetValueN(PS, "page/@height")
    srcl/S: End If
    srcl/S: rad_page_inches.Value = True
    srcl/S: End If
    srcl/S: edt_page_top.EnforceRange = True
    srcl/S: edt_page_left.EnforceRange = True
    srcl/S: edt_page_right.EnforceRange = True
    srcl/S: edt_page_bottom.EnforceRange = True
    srcl/S: Initializing = False
    srcl/S: 
    srcl/S: // Mac and Windows use PrinterSetup for page size.
    srcl/S: #If TargetMacOS Or TargetWin32
    srcl/S: edt_page_width.ReadOnly = True
    srcl/S: edt_page_height.ReadOnly = True
    srcl/S: #Else
    srcl/S: edt_page_width.ReadOnly = False
    srcl/S: edt_page_height.ReadOnly = False
    srcl/S: #Endif
    srcl/S: 
    srcl/S: can_font_title.SetFont SmartML.GetValueF(PS, "title")
    srcl/S: can_font_author.SetFont SmartML.GetValueF(PS, "author")
    srcl/S: can_font_sections.SetFont SmartML.GetValueF(PS, "heading")
    srcl/S: can_font_lyrics.SetFont SmartML.GetValueF(PS, "lyrics")
    srcl/S: can_font_chords.SetFont SmartML.GetValueF(PS, "chords")
    srcl/S: can_font_capo.SetFont SmartML.GetValueF(PS, "capo_chords")
    srcl/S: can_font_copyright.SetFont SmartML.GetValueF(PS, "copyright")
    srcl/S: 
    srcl/S: chk_layout_title_in_caps.Value = SmartML.GetValueB(PS, "style/@caps_song_title")
    srcl/S: edt_layout_heading_border_thickness.Text = SmartML.GetValue(PS, "style/@border_thickness")
    srcl/S: 
    srcl/S: App.T.TranslateWindow Me, "print_settings", App.TranslationFonts
    srcl/S: App.CenterInControlScreen Me
    srcl/S: End Sub
