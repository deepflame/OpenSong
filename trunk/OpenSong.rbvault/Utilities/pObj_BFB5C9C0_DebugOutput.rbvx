  parm/S: 
  rslt/S: Integer
  flag/N: 0
  sorc/d: (Enco/N: 134217984
		Function MoreDetail() As Integer
		  If DebugLevel < MAXDEBUGLEVEL Then DebugLevel = DebugLevel + 1
		  Return DebugLevel
		End Function
  )
