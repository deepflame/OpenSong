  sorc/d: (Enco/N: 134217984
		Sub Constructor()
		  DebugTOS = Nil
		  DebugFile = Nil
		  Enabled = False
		  If System.EnvironmentVariable("OPENSONGDEBUGLEVEL") = "" Then
		    DebugLevel = 1
		  Else
		    DebugLevel = CDbl(System.EnvironmentVariable("OPENSONGDEBUGLEVEL"))
		  End If
		  '++JRC
		  AppendLog = True
		  '--
		End Sub
  )
